             ,{nationality = "FR", name = "Melanie"}
             ,{nationality = "DE", name = "Torsten"}
             ,{nationality = "IT", name = "Manuel"}
             ,{nationality = "NL", name = "Jeroen"}]
 in groupBy1 (\p -> {name = p.name} ) (\x -> x.nationality) people